{
  "name": "euphio",
  "productName": "Euphio",
  "version": "1.1.4",
  "description": "A Bandcamp scraper for finding new albums tagged with multiple specified tag combinations.",
  "main": "src/index.ts",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "set-tag-nix": "git tag $npm_package_version",
    "set-tag-win": "git tag %npm_package_version%",
    "publish": "electron-forge publish",
    "lint": "tslint src"
  },
  "keywords": [],
  "author": "cmgriffing",
  "license": "MIT",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "yarn"
      },
      "electronWinstallerConfig": {
        "name": "euphio"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "Euphio"
      }
    }
  },
  "dependencies": {
    "@angular/animations": "^6.0.5",
    "@angular/cdk": "^6.2.1",
    "@angular/common": "^6.0.4",
    "@angular/compiler": "^6.0.4",
    "@angular/core": "^6.0.4",
    "@angular/flex-layout": "6.0.0-beta.15",
    "@angular/forms": "^6.0.5",
    "@angular/material": "^6.2.1",
    "@angular/platform-browser": "^6.0.4",
    "@angular/platform-browser-dynamic": "^6.0.4",
    "bandcamp-tag-scraper": "git+https://github.com/cmgriffing/bandcamp-tag-scraper.git#bd826e8",
    "electron-compile": "^6.4.2",
    "electron-devtools-installer": "^2.1.0",
    "electron-squirrel-startup": "^1.0.0",
    "randomcolor": "^0.5.3",
    "reflect-metadata": "^0.1.9",
    "rxjs": "^6.0.0",
    "rxjs-compat": "^6.0.0",
    "tslib": "^1.4.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "electron-forge": "^5.2.2",
    "electron-icon-maker": "0.0.4",
    "electron-local-crash-reporter": "^1.0.2",
    "electron-prebuilt-compile": "2.0.2",
    "node-sass": "^4.9.0",
    "tslint": "^4.2.0",
    "typescript": "~2.1.4"
  }
}
